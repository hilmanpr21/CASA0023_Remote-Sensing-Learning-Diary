@article{zhangTropicalCycloneMotion2020,
  title = {Tropical Cyclone Motion in a Changing Climate},
  author = {Zhang, Gan and Murakami, Hiroyuki and Knutson, Thomas R. and Mizuta, Ryo and Yoshida, Kohei},
