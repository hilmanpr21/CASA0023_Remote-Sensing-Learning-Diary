@article{eckmannEstimatingSubpixelFire2009,
  title = {Estimating Subpixel Fire Sizes and Temperatures from {{ASTER}} Using Multiple Endmember Spectral Mixture Analysis},
  author = {Eckmann, T. C. and Roberts, D. A. and Still, C. J.},
  date = {2009-10-21},
  journaltitle = {International Journal of Remote Sensing},
  volume = {30},
