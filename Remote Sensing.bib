@article{millerHoningBioluminescentMilky2021,
  title = {Honing in on Bioluminescent Milky Seas from Space},
  author = {Miller, Steven D. and Haddock, Steven H. D. and Straka, William C. and Seaman, Curtis J. and Combs, Cynthia L. and Wang, Menghua and Shi, Wei and Nam, SungHyun},
